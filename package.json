    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "husky": "^8.0.1",
    "postcss-html": "^1.4.1",
    "prettier": "^2.6.2",
    "prettier-plugin-svelte": "^2.7.0",
    "stylelint": "^14.8.5",
    "stylelint-config-recommended": "^7.0.0",
